--// LUXUSWQ AC6 PLAYER PINK FULL

local loopState = true

local NotificationLibrary =
loadstring(game:HttpGet(
"https://raw.githubusercontent.com/Roblox-HttpSpy/AC6-Music-Exploit/refs/heads/main/notif.luau"))()

local Workspace = game:GetService("Workspace")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Lighting = game:GetService("Lighting")
local UserInputService = game:GetService("UserInputService")

local InjectTo = game:GetService("TestService")

-- remote finder
local function findRemoteEvent(name,parent)
local r = parent:QueryDescendants(">>RemoteEvent#" .. name)
return r[1]
end

local function getRemoteEvent()
return findRemoteEvent("AC6_FE_Sounds",Workspace)
or findRemoteEvent("AC6_FE_Sounds",ReplicatedStorage)
end

-- hide gui
local function Hide()
local s,r=pcall(gethui)
return s and r or game.CoreGui
end

local guiParent = Hide()

-- random
local chars="abcdefghijklmnopqrstuvwxyz"

local function rand()
local s=""
for i=1,8 do
s=s..chars:sub(math.random(1,#chars),math.random(1,#chars))
end
return s
end

-- blur
local blur=Instance.new("BlurEffect",Lighting)
blur.Size=10

-- gui
local gui=Instance.new("ScreenGui",guiParent)
gui.Name=rand()
gui.ResetOnSpawn=false

-- bubble
local bubble=Instance.new("TextButton",gui)
bubble.Size=UDim2.new(0,60,0,60)
bubble.Position=UDim2.new(0,20,0.5,-30)
bubble.Text="OPEN"
bubble.BackgroundColor3=Color3.fromRGB(255,105,180)
bubble.Visible=false
Instance.new("UICorner",bubble)

-- main
local main=Instance.new("Frame",gui)
main.Size=UDim2.new(0,420,0,260)
main.Position=UDim2.new(0.5,-210,0.5,-130)

-- PEMBE ARKA PLAN
main.BackgroundColor3=Color3.fromRGB(255,105,180)

Instance.new("UICorner",main)

-- stroke
local stroke=Instance.new("UIStroke",main)
stroke.Thickness=2

local grad=Instance.new("UIGradient",stroke)

grad.Color=ColorSequence.new{

ColorSequenceKeypoint.new(0,Color3.fromRGB(255,255,255)),
ColorSequenceKeypoint.new(1,Color3.fromRGB(255,0,255))

}

task.spawn(function()
while true do
grad.Rotation+=1
task.wait()
end
end)

-- title
local title=Instance.new("TextLabel",main)
title.Size=UDim2.new(1,0,0,40)
title.BackgroundTransparency=1
title.Text="LUXUSWQ HUB"
title.Font=Enum.Font.GothamBold
title.TextSize=24

-- minimize
local mini=Instance.new("TextButton",main)
mini.Size=UDim2.new(0,30,0,30)
mini.Position=UDim2.new(1,-70,0,5)
mini.Text="-"

-- close
local close=Instance.new("TextButton",main)
close.Size=UDim2.new(0,30,0,30)
close.Position=UDim2.new(1,-35,0,5)
close.Text="X"

-- text
local textBox=Instance.new("TextBox",main)
textBox.Size=UDim2.new(0.8,0,0,40)
textBox.Position=UDim2.new(0.1,0,0.3,0)
textBox.PlaceholderText="Music ID"

-- volume
local volumeBox=textBox:Clone()
volumeBox.Parent=main
volumeBox.Position=UDim2.new(0.1,0,0.5,0)
volumeBox.Size=UDim2.new(0.35,0,0,35)
volumeBox.PlaceholderText="Volume"

-- pitch
local pitchBox=volumeBox:Clone()
pitchBox.Parent=main
pitchBox.Position=UDim2.new(0.55,0,0.5,0)
pitchBox.PlaceholderText="Pitch"

-- play
local play=Instance.new("TextButton",main)
play.Size=UDim2.new(0.4,0,0,40)
play.Position=UDim2.new(0.3,0,0.75,0)
play.Text="PLAY"

-- minimize function
mini.MouseButton1Click:Connect(function()
main.Visible=false
bubble.Visible=true
end)

-- close function
close.MouseButton1Click:Connect(function()
gui:Destroy()
blur:Destroy()
end)

-- open
bubble.MouseButton1Click:Connect(function()
main.Visible=true
bubble.Visible=false
end)

-- DRAG

local drag=false
local dragStart
local startPos

main.InputBegan:Connect(function(input)

if input.UserInputType==Enum.UserInputType.MouseButton1 then

drag=true
dragStart=input.Position
startPos=main.Position

end

end)

UserInputService.InputChanged:Connect(function(input)

if drag and input.UserInputType==Enum.UserInputType.MouseMovement then

local delta=input.Position-dragStart

main.Position=UDim2.new(
startPos.X.Scale,
startPos.X.Offset+delta.X,
startPos.Y.Scale,
startPos.Y.Offset+delta.Y)

end

end)

UserInputService.InputEnded:Connect(function(input)
if input.UserInputType==Enum.UserInputType.MouseButton1 then
drag=false
end
end)

-- play system
local sound=rand()

play.MouseButton1Click:Connect(function()

local id=textBox.Text
local vol=tonumber(volumeBox.Text) or 1
local pitch=tonumber(pitchBox.Text) or 1

local asset="rbxassetid://"..id

pcall(function()

local remote=getRemoteEvent()

remote:FireServer("newSound",sound,InjectTo,asset,pitch,vol,loopState)
remote:FireServer("playSound",sound)

NotificationLibrary:SendNotification(
"LUXUSWQ",
"PLAYED",
3)

end)

end)
