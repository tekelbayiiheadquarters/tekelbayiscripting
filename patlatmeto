--// LUXUSWQ PINK HUB FULL FIXED

local loopState = true

local NotificationLibrary =
loadstring(game:HttpGet(
"https://raw.githubusercontent.com/Roblox-HttpSpy/AC6-Music-Exploit/refs/heads/main/notif.luau"))()

local Workspace = game:GetService("Workspace")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local UserInputService = game:GetService("UserInputService")

local InjectTo = game:GetService("TestService")

-- GUI PARENT
local parent =
pcall(function() return gethui() end)
and gethui()
or game.CoreGui

-- FIND REMOTE
local function find(name,parent)
local r = parent:QueryDescendants(">>RemoteEvent#" .. name)
return r[1]
end

local function remote()
return find("AC6_FE_Sounds",Workspace)
or find("AC6_FE_Sounds",ReplicatedStorage)
end

-- RANDOM
local function rand()
return tostring(math.random(10000,99999))
end

-- GUI
local gui = Instance.new("ScreenGui",parent)

-- BUBBLE
local bubble = Instance.new("TextButton",gui)

bubble.Size = UDim2.new(0,70,0,70)
bubble.Position = UDim2.new(0,20,0.5,0)

bubble.BackgroundColor3 = Color3.fromRGB(255,0,150)

bubble.Text = "OPEN"

bubble.Visible = false

Instance.new("UICorner",bubble)

-- MAIN FRAME
local main = Instance.new("Frame",gui)

main.Size = UDim2.new(0,420,0,260)
main.Position = UDim2.new(0.5,-210,0.5,-130)

-- ðŸ’Ž NET PEMBE
main.BackgroundColor3 = Color3.fromRGB(255,20,147)

Instance.new("UICorner",main)

-- TITLE
local title = Instance.new("TextLabel",main)

title.Size = UDim2.new(1,0,0,40)
title.BackgroundTransparency = 1
title.Text = "LUXUSWQ"
title.TextScaled = true

-- MINIMIZE
local mini = Instance.new("TextButton",main)

mini.Size = UDim2.new(0,30,0,30)
mini.Position = UDim2.new(1,-70,0,5)

mini.Text = "-"

-- CLOSE
local close = Instance.new("TextButton",main)

close.Size = UDim2.new(0,30,0,30)
close.Position = UDim2.new(1,-35,0,5)

close.Text = "X"

-- MUSIC BOX
local music = Instance.new("TextBox",main)

music.Size = UDim2.new(0.8,0,0,40)
music.Position = UDim2.new(0.1,0,0.3,0)

music.PlaceholderText = "Music ID"

-- PLAY
local play = Instance.new("TextButton",main)

play.Size = UDim2.new(0.4,0,0,40)
play.Position = UDim2.new(0.3,0,0.7,0)

play.Text = "PLAY"

-- MINIMIZE FUNCTION
mini.MouseButton1Click:Connect(function()

main.Visible = false
bubble.Visible = true

end)

-- OPEN
bubble.MouseButton1Click:Connect(function()

main.Visible = true
bubble.Visible = false

end)

-- CLOSE
close.MouseButton1Click:Connect(function()

gui:Destroy()

end)



local dragging = false
local dragStart
local startPos

main.InputBegan:Connect(function(input)

if input.UserInputType == Enum.UserInputType.MouseButton1 then

dragging = true

dragStart = input.Position

startPos = main.Position

end

end)

UserInputService.InputChanged:Connect(function(input)

if dragging then

local delta = input.Position - dragStart

main.Position = UDim2.new(

startPos.X.Scale,
startPos.X.Offset + delta.X,

startPos.Y.Scale,
startPos.Y.Offset + delta.Y

)

end

end)

UserInputService.InputEnded:Connect(function()

dragging = false

end)

-- PLAY SYSTEM

local sound = rand()

play.MouseButton1Click:Connect(function()

local id = music.Text

local asset = "rbxassetid://" .. id

pcall(function()

local r = remote()

r:FireServer(

"newSound",
sound,
InjectTo,
asset,
1,
1,
true

)

r:FireServer("playSound",sound)

NotificationLibrary:SendNotification(
"SÄ°KÄ°ÅžÄ°STAN",
"PLAYING",
3)

end)

end)
