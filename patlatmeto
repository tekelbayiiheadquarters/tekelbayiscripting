--// LUXUSWQ MODERN PINK HUB

local loopState = true

local NotificationLibrary =
loadstring(game:HttpGet(
"https://raw.githubusercontent.com/Roblox-HttpSpy/AC6-Music-Exploit/refs/heads/main/notif.luau"))()

local UIS = game:GetService("UserInputService")
local Workspace = game:GetService("Workspace")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

local InjectTo = game:GetService("TestService")

local parent = pcall(function() return gethui() end)
and gethui() or game.CoreGui

-- remote
local function getRemote()
local function find(name,p)
local r=p:QueryDescendants(">>RemoteEvent#" .. name)
return r[1]
end
return find("AC6_FE_Sounds",Workspace)
or find("AC6_FE_Sounds",ReplicatedStorage)
end

-- random
local function rand()
return tostring(math.random(100000,999999))
end

-- GUI
local gui = Instance.new("ScreenGui",parent)
gui.ResetOnSpawn=false

-- bubble
local bubble = Instance.new("TextButton",gui)
bubble.Size=UDim2.new(0,60,0,60)
bubble.Position=UDim2.new(0,20,0.5,0)
bubble.Text="L"
bubble.Visible=false

bubble.Font=Enum.Font.GothamBold
bubble.TextSize=28
bubble.TextColor3=Color3.new(1,1,1)

bubble.BackgroundColor3=Color3.fromRGB(255,105,180)

Instance.new("UICorner",bubble).CornerRadius=UDim.new(1,0)

-- main
local main=Instance.new("Frame",gui)
main.Size=UDim2.new(0,420,0,320)
main.Position=UDim2.new(0.5,-210,0.5,-160)

main.BackgroundColor3=Color3.fromRGB(255,182,193)

Instance.new("UICorner",main).CornerRadius=UDim.new(0,12)

-- shadow
local stroke=Instance.new("UIStroke",main)
stroke.Color=Color3.fromRGB(255,105,180)
stroke.Thickness=2

-- title
local title=Instance.new("TextLabel",main)
title.Size=UDim2.new(1,0,0,50)
title.BackgroundTransparency=1

title.Text="LUXUSWQ HUB"

title.Font=Enum.Font.GothamBold
title.TextSize=28

title.TextColor3=Color3.fromRGB(40,40,40)

-- minimize
local mini=Instance.new("TextButton",main)
mini.Size=UDim2.new(0,30,0,30)
mini.Position=UDim2.new(1,-70,0,10)

mini.Text="-"
mini.Font=Enum.Font.GothamBold
mini.TextSize=18

-- close
local close=Instance.new("TextButton",main)
close.Size=UDim2.new(0,30,0,30)
close.Position=UDim2.new(1,-35,0,10)

close.Text="X"
close.Font=Enum.Font.GothamBold

-- textbox function
local function createBox(y,text)

local box=Instance.new("TextBox",main)

box.Size=UDim2.new(0.8,0,0,40)
box.Position=UDim2.new(0.1,0,y,0)

box.PlaceholderText=text

box.Font=Enum.Font.Gotham
box.TextSize=16

box.BackgroundColor3=Color3.fromRGB(255,240,245)

Instance.new("UICorner",box)

return box

end

local music=createBox(0.2,"Music ID")
local volume=createBox(0.38,"Volume")
local pitch=createBox(0.56,"Pitch")

-- loop button
local loopBtn=Instance.new("TextButton",main)

loopBtn.Size=UDim2.new(0.8,0,0,35)
loopBtn.Position=UDim2.new(0.1,0,0.72,0)

loopBtn.Text="Loop : ON"

loopBtn.Font=Enum.Font.GothamBold
loopBtn.TextSize=16

loopBtn.BackgroundColor3=Color3.fromRGB(255,105,180)
loopBtn.TextColor3=Color3.new(1,1,1)

Instance.new("UICorner",loopBtn)

loopBtn.MouseButton1Click:Connect(function()

loopState=not loopState

loopBtn.Text="Loop : "..(loopState and "ON" or "OFF")

end)

-- play button
local play=Instance.new("TextButton",main)

play.Size=UDim2.new(0.5,0,0,45)
play.Position=UDim2.new(0.25,0,0.85,0)

play.Text="PLAY"

play.Font=Enum.Font.GothamBold
play.TextSize=18

play.BackgroundColor3=Color3.fromRGB(255,20,147)
play.TextColor3=Color3.new(1,1,1)

Instance.new("UICorner",play)

-- drag
local drag=false
local dragStart,startPos

main.InputBegan:Connect(function(input)

if input.UserInputType==Enum.UserInputType.MouseButton1 then

drag=true
dragStart=input.Position
startPos=main.Position

end

end)

UIS.InputChanged:Connect(function(input)

if drag then

local delta=input.Position-dragStart

main.Position=UDim2.new(

startPos.X.Scale,
startPos.X.Offset+delta.X,
startPos.Y.Scale,
startPos.Y.Offset+delta.Y)

end

end)

UIS.InputEnded:Connect(function()
drag=false
end)

-- minimize
mini.MouseButton1Click:Connect(function()

main.Visible=false
bubble.Visible=true

end)

bubble.MouseButton1Click:Connect(function()

main.Visible=true
bubble.Visible=false

end)

-- close
close.MouseButton1Click:Connect(function()
gui:Destroy()
end)

-- play
local sound=rand()

play.MouseButton1Click:Connect(function()

local id=music.Text
local vol=tonumber(volume.Text) or 1
local pit=tonumber(pitch.Text) or 1

local asset="rbxassetid://"..id

pcall(function()

local r=getRemote()

r:FireServer(
"newSound",
sound,
InjectTo,
asset,
pit,
vol,
loopState)

r:FireServer("playSound",sound)

NotificationLibrary:SendNotification(
"LUXUSWQ",
"Playing",
3)

end)

end)
